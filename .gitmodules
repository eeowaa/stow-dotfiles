# Heavy "daily driver" submodules
[submodule "org"]
	path = org/org
	url = git@github.com:eeowaa/org.git
	branch = main
[submodule "doom-emacs"]
	path = doom/.config/doom
	url = git@github.com:eeowaa/doomdir.git
	branch = main
[submodule "vim-packages"]
	path = vim/.vim/pack/eeowaa
	url = git@github.com:eeowaa/vim-plugins.git
	branch = master

# Tool collections
[submodule "cygtools"]
	path = cygwin/.local/opt/cygtools
	url = git@github.com:eeowaa/cygtools.git
	branch = master
[submodule "mailconvert"]
	path = utils/.local/opt/mailconvert
	url = git@github.com:eeowaa/mailconvert.git
	branch = master

# Reference repos
[submodule "logparse"]
	path = .stash/.local/opt/logparse
	url = git@github.com:eeowaa/logparse.git
	branch = master
[submodule "jackwagon"]
	path = .stash/.local/opt/jackwagon
	url = git@github.com:eeowaa/jackwagon.git
	branch = master
[submodule "vanilla-emacs"]
	path = .stash/.config/emacs
	url = git@github.com:eeowaa/vanilla-emacs.git
	branch = doom-migration

# Reference Doom Emacs configurations
[submodule "hlissner/doom-emacs-private"]
	path = doom/.local/src/doom/hlissner/doom-emacs-private
	url = git@github.com:hlissner/doom-emacs-private.git
	branch = master
[submodule "tecosaur/emacs-config"]
	path = doom/.local/src/doom/tecosaur/emacs-config
	url = git@github.com:tecosaur/emacs-config.git
	branch = master

# For native-comp Emacs on macOS
[submodule "build-emacs-for-macos"]
	path = emacs/.local/src/emacs/build-emacs-for-macos
	url = git@github.com:jimeh/build-emacs-for-macos.git
	branch = master
